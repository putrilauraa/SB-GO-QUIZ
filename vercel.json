{
    "builds": [
        {
            "src": "api/*.go",
            "use": "@vercel/go"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.go"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.go"
        }
    ]
}